<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Scheduler -->
        <record forcecreate="True" id="bmair_ir_cron_scheduler_action" model="ir.cron">
            <field name="name">Procurement Manual</field>
            <field name="model_id" ref="stock.model_procurement_group"/>
            <field name="state">code</field>
            <field name="code">
model.procurement_manual_run_scheduler(True)
            </field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>

    </data>
</odoo>
